{
  "name": "gym-intelligence-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev --webpack",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:push": "prisma db push",
    "db:push:local": "dotenv -e .env.local -- prisma db push",
    "db:push:staging": "dotenv -e .env.staging -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:local": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:staging": "dotenv -e .env.staging -- prisma migrate deploy",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:dev": "TARGET_DB=dev tsx prisma/seed.ts",
    "db:seed:staging": "TARGET_DB=staging tsx prisma/seed.ts",
    "db:seed-gym": "tsx prisma/seed-gym.ts",
    "db:seed:prod": "tsx prisma/seed-prod.ts",
    "db:studio": "prisma studio",
    "db:studio:local": "dotenv -e .env.local -- prisma studio",
    "db:studio:staging": "dotenv -e .env.staging -- prisma studio",
    "db:studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:reset": "prisma migrate reset",
    "db:reset:local": "dotenv -e .env.local -- prisma migrate reset",
    "db:reset:staging": "dotenv -e .env.staging -- prisma migrate reset",
    "db:reset:prod": "echo '⚠️  DANGER: This will reset PRODUCTION database!' && dotenv -e .env.production -- prisma migrate reset",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "^6.19.1",
    "@stripe/stripe-js": "^8.6.0",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.3",
    "jose": "^6.1.3",
    "next": "16.1.1",
    "prisma": "^6.19.1",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-image-crop": "^11.0.10",
    "recharts": "^3.6.0",
    "stripe": "^20.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "dotenv-cli": "^8.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "utf-8-validate": "^6.0.6"
  }
}
